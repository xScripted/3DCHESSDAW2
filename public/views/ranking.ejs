<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="perfil.css">
    <link href="https://fonts.googleapis.com/css?family=Montserrat|Raleway:900" rel="stylesheet"> 
    <script src="Javascript/TweenMax.min.js"></script>
</head>
<style>
    body {
        background-color: black;
        overflow: hidden;
    }
    #ranking {
        position: relative;
        margin: auto;
        width: 70%;
        height: 100%;
        background-color: white;
    }
    table {
        width: 100%;  
        font-family: Montserrat, sans-serif;
        font-size: 15px;      
    }
    td, th {
        padding: 20px;
    }
    th {
        text-align: left;
    }
    #particles {
        position: absolute;
        width: 100%;
        height: 1000px;
    }
</style>
<body>    
    <div id="particles"></div>
    <script src="../Javascript/particles.js"></script>
    <div id="menu">
            <div> PERFIL </div>      
            <div> RANKING </div>   
            <div> HOME </div>
            <div> LOGIN</div>
            <div> LOGOUT</div>
        </div>
    <div id="ranking">
        <table>
            <tr>
                <th>Nombre</th>
                <th>ELO</th>
                <th>Victorias</th>
                <th>Derrotas</th>
                <th>Empates</th>
                <th>Tiempo jugado</th>
            </tr>
            <% for(var x of data) {%>
                <tr><td><%= x.nick %></td><td><%= x.elo %></td><td><%= x.victorias %></td><td><%= x.derrotas %></td><td><%= x.empates %></td><td><%= x.time %></td>  <% } %></tr>
        </table>
    </div>
</body>
</html>